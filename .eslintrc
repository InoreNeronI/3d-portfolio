{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "eslint-config-prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "react/no-unescaped-entities": "warn",
        "react/no-unknown-property": "warn",
        "react/prop-types": "warn",
        "react/react-in-jsx-scope": "warn"
    }
}
